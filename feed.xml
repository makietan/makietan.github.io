      attack: 28.5 + energyBoost + (isWithin4 ? 35 : 0),
      defense: 28.5 + energyBoost + (isWithin4 ? 35 : 0),
      accuracy: 28.5 + energyBoost + (isWithin4 ? 25 : 0),
      mobility: 0
